<div class="page-header">
  <h1>
    Tag catalogues
    <a href="/tag_catalogues/new" class="btn btn-lg btn-success">New Tag catalogue</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Tag content</th>
        <th>Actions</th>
      </tr>

      <% @tag_catalogues.each do |tag_catalogue| %>
      <tr>
        <td><%= tag_catalogue.tag_content %></td>
        <td>
          <a href="/tag_catalogues/<%= tag_catalogue.id %>" class="btn btn-primary">Show</a>
          <a href="/tag_catalogues/<%= tag_catalogue.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_tag_catalogue/<%= tag_catalogue.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
