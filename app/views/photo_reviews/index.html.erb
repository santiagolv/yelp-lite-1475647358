<div class="page-header">
  <h1>
    Photo reviews
    <a href="/photo_reviews/new" class="btn btn-lg btn-success">New Photo review</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Review</th>
        <th>Photos</th>
        <th>Actions</th>
      </tr>

      <% @photo_reviews.each do |photo_review| %>
      <tr>
        <td><%= photo_review.review_id %></td>
        <td><%= photo_review.photos %></td>
        <td>
          <a href="/photo_reviews/<%= photo_review.id %>" class="btn btn-primary">Show</a>
          <a href="/photo_reviews/<%= photo_review.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_photo_review/<%= photo_review.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
