<div class="page-header">
  <h3>Photo Review #<%= @photo_review.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Review</dt>
      <dd><%= @photo_review.review_id %></dd>

      <dt>Photos</dt>
      <dd><%= @photo_review.photos %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/photo_reviews" class="btn btn-primary">
        Back
      </a>
      <a href="/photo_reviews/<%= @photo_review.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_photo_review/<%= @photo_review.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
