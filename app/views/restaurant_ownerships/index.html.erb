<div class="page-header">
  <h1>
    Restaurant ownerships
    <a href="/restaurant_ownerships/new" class="btn btn-lg btn-success">New Restaurant ownership</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Restaurant</th>
        <th>Actions</th>
      </tr>

      <% @restaurant_ownerships.each do |restaurant_ownership| %>
      <tr>
        <td><%= restaurant_ownership.user.name %></td>
        <td><%= restaurant_ownership.restaurant_id %></td>
        <td>
          <a href="/restaurant_ownerships/<%= restaurant_ownership.id %>" class="btn btn-primary">Show</a>
          <a href="/restaurant_ownerships/<%= restaurant_ownership.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_restaurant_ownership/<%= restaurant_ownership.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
